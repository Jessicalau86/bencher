---
import DocsPanel from "../../../components/docs/DocsPanel.astro";
import { Language, otherLanguages } from "../../../i18n/ui";

export async function getStaticPaths() {
    return otherLanguages.map((lang) => {return { params: { lang } }});
}

const lang: Language = Astro.params.lang;
---

<DocsPanel lang={lang}/>
