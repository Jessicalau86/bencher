---
export const prerender = false;

import PerfLayout from "../../layouts/perf/PerfLayout.astro";
import PublicProject from "../../components/perf/PublicProject";

const BENCHER_API_URL = import.meta.env.BENCHER_API_URL;
const params = Astro.params;
const title = "Project Perf";
---

<PerfLayout
  title={title}
  description="View benchmark performance of this public project on Bencher"
>
  <PublicProject client:only apiUrl={BENCHER_API_URL} params={params} />
</PerfLayout>
